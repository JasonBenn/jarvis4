{
  "name": "readwise-uploader",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "upload": "tsx src/upload-to-readwise.ts",
    "db:init": "prisma generate --schema=db/schema.prisma && prisma db push --schema=db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=db/schema.prisma",
    "db:reset": "prisma migrate reset --schema=db/schema.prisma",
    "db:studio": "prisma studio --schema=db/schema.prisma",
    "ext:db": "sqlite3 \"$HOME/Library/Application Support/Cursor/User/globalStorage/jasonbenn.jarvis4-worldview-updater/readwise-highlights.db\"",
    "ext:db:metadata": "pnpm run ext:db \"SELECT * FROM metadata;\"",
    "ext:db:stats": "pnpm run ext:db \"SELECT status, COUNT(*) as count FROM highlight_states GROUP BY status; SELECT COUNT(*) as total_data FROM highlight_data;\"",
    "install-alias": "./src/install-aliases.sh"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "dotenv": "^16.4.7",
    "marked": "^15.0.4",
    "openai": "^6.1.0",
    "prisma": "^6.16.3"
  }
}

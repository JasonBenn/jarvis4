{
  "name": "readwise-uploader",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "upload": "tsx src/upload-to-readwise.ts",
    "db:init": "prisma generate --schema=db/schema.prisma && prisma db push --schema=db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=db/schema.prisma",
    "db:reset": "prisma migrate reset --schema=db/schema.prisma",
    "db:studio": "prisma studio --schema=db/schema.prisma",
    "install-alias": "grep -q 'alias worldview-upload=' ~/.bash_profile 2>/dev/null && sed -i '' '/alias worldview-upload=/d' ~/.bash_profile; echo \"alias worldview-upload='cd $PWD && pnpm upload'\" >> ~/.bash_profile && echo 'Alias installed! Run: source ~/.bash_profile'"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "dotenv": "^16.4.7",
    "marked": "^15.0.4",
    "openai": "^6.1.0",
    "prisma": "^6.16.3"
  }
}
